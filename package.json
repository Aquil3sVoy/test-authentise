    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/react-redux": "^7.1.24",
    "axios": "^0.27.2",
    "connected-react-router": "6.8.0",
    "postcss-cli": "^9.1.0",
    "postcss-preset-env": "^7.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:ui": "npm run start:localhost",
    "start:localhost": "cross-env REACT_APP_ENVIRONMENT=localhost craco start",
    "build": "react-scripts build",
    "test": "cross-env REACT_APP_ENVIRONMENT=localhost react-scripts test",
    "test:coverage": "cross-env REACT_APP_ENVIRONMENT=localhost react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-named-as-default": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.7",
    "cross-env": "7.0.3",
    "less": "4.0.0",
    "msw": "^0.44.2",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "config.ts",
      "index.ts",
      "public/*",
      "./*.d.ts",
      "App.tsx",
      "serviceWorker.ts",
      "./modules/base/baseActions.tsx",
      "./modules/base/baseService.tsx",
      "./modules/hooks/CustomHoook.tsx"
